{
    "manifest_version": 2,
    "name": "Redirect Redmine Login to Google Authorizathion",
    "description": "",
    "version": "1.0",
    "page_action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["https://redmine.fout.jp/login*"],
            "js":["js/redirect-login.js"]
        }
    ],
    "permissions": [
        "storage"
    ]
}
